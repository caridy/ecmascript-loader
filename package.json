{
	"name": "ecmascript-loader",
	"version": "1.0.0",
	"description": "ES7 spec proposal for Reflect.Loader and Reflect.Module (uplifted from whatwg/loader proposal)",
	"private": true,
	"scripts": {
		"build": "ecmarkup spec.emu --old-toc --js=ecmarkup.js --css=ecmarkup.css spec.html",
		"watch": "ecmarkup spec.emu --old-toc --watch --js=ecmarkup.js --css=ecmarkup.css spec.html",
		"prepublish": "npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/caridy/ecmascript-loader.git"
	},
	"keywords": [
		"Reflect.Loader",
		"Reflect.Module",
		"loader",
		"modules",
		"ES7",
		"spec"
	],
	"author": "Caridy Pati√±o <caridy@gmail.com>",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/caridy/ecmascript-loader/issues"
	},
	"homepage": "https://github.com/caridy/ecmascript-loader#readme",
	"dependencies": {},
	"devDependencies": {
		"ecmarkup": "^2.2.0",
		"js-beautify": "^1.5.10",
		"tape": "^4.2.0"
	}
}
